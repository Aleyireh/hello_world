#include <iostream>
using namespace std;

int main() {
    float base, altura, area;

    cout << "Ingresa la base del rectangulo: ";
    cin >> base;

    cout << "Ingresa la altura del rectangulo: ";
    cin >> altura;

    area = base * altura;

    cout << "El area del rectangulo es: " << area << endl;

    return 0;
}
